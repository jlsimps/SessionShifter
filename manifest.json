{
    "manifest_version": 3,
    "name": "Session Manager v1",
    "version": "1.0",
    "description": "Manage multiple isolated sessions",
    "permissions": ["cookies", "storage", "tabs", "webNavigation"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "host_permissions": ["<all_urls>"]
  }
  